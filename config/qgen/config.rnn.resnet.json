{
  "name" : "Baseline : RNN++",

  "model": {

    "type" : "rnn",

    "inputs": {
      "image": true
    },

    "dialogue": {
      "word_embedding_dim": 512,
      "cell": "gru",
      "rnn_units": 1024,
      "layer_norm" : true
    },

    "image": {
      "image_input": "conv",
      "dim": [14, 14, 1024],
      "normalize": false,
      "projection_units" : 512
    },

    "pooling" : {
      "mode": "mean"
    }
  },

  "optimizer": {
    "no_epoch": 10,
    "learning_rate": 1e-4,
    "batch_size": 64,
    "clip_val": 5.0,
    "weight_decay": 0,
    "weight_decay_add": [],
    "weight_decay_remove": []
  },

  "status": ["success"],
  "seed": -1
}