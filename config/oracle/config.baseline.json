{
  "name" : "Baseline",

  "model": {

    "type": "baseline",

    "inputs": {
      "question": true,
      "category": true,
      "spatial": true,
      "crop": false,
      "image": false
    },

    "question": {
      "input_type": "question",
      "word_embedding_dim": 300,
      "glove": false,
      "cell": "lstm",
      "rnn_units": 512
    },

    "category": {
      "n_categories": 90,
      "embedding_dim": 512
    },

    "MLP": {
      "num_hiddens": 512
    },

    "image": {
      "image_input": "fc8",
      "dim": [1000]
    },

    "crop": {
      "image_input": "fc8",
      "dim": [1000],
      "scale" : 1.1
    },

    "pooling" : {
      "mode": "none"
    },

    "dropout_keep_prob" : 1.0

  },

  "optimizer": {
    "no_epoch": 20,
    "learning_rate": 1e-4,
    "batch_size": 64,
    "clip_val": 5,
    "weight_decay": 0,
    "weight_decay_add": [],
    "weight_decay_remove": []
  },

  "status": ["success"],
  "seed": -1

}


